thing MyInnerThing <| {
  im_a_float: float
}

thing MyThing <| {
  inner: MyInnerThing,
  im_an_int: int
}

pipe main |> [] |> [] |> unit {


  mut MyThing t <| MyThing {
    inner: MyInnerThing {
      im_a_float: 5.1
    },
    im_an_int: 5
  };

  ~&float x <| t.inner.im_a_float;

  Printnl <| [x];

  |> ();
}