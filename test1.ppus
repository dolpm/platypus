thing CloudConsciousness <| {
  enemies : vector[bool]
}

pipe transfigure |> [] |> [subject : CloudConsciousness] |> unit {
  &vector[bool] to_be_enlightened <| &subject[enemies];

  |> ();
}

pipe main |> [] |> [] |> unit {
  mut [vector[int] * box[bool]] brainfolds <| tuple(Vector_new <| [], Box_new <| [true]);

  ~&vector[int] memories <| ~&brainfolds[0];

  mut CloudConsciousness mortality <| CloudConsciousness {
    enemies : Vector_new <| []
  };

  transfigure <| [mortality];

  |> ();
}