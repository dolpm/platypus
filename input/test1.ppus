thing MyThing <| {
  im_a_string: string,
  im_an_int: int
}

pipe fib |> ['a] |> [n: &'a int] |> int {
  
  if (n < 0) {
    panic <| ["invalid_input!"];
  }
  
  if (n == 0) {
    |> 0;
  } else if (n == 1 && n == 2) {
    |> 1;
  }

  |> (fib <| [n - 1]) + (fib <| [n - 2]);
}

pipe main |> [] |> [] |> unit {
  loop 0 -> 5 as i {
    printnl <| [int_to_string <| [fib <| [i]]];
  }

  int x <| 0;
  while x < 5 {
    x <| (x + 1);
  }

  |> ();
}