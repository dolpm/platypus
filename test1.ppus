pipe test |'a, 'b, 'c| [x: &'a int, y: &'b int, z: &'c int] |> &'c int {
  if (@x == 2) {
    |> x;
  }
  if (@y == 2) {
    |> y;
  }
  |> z;
}

pipe main [] |> unit {
  mut int x = 4;
  while (x < 5) {
    x = x + 1;
    int y = 4;
    loop 0 -> 5 as z {
      test <| [&x, &y, &z];
    }
    int w = 10;
  }
  |> ();
}
