pipe test |> ['a, 'b, 'c] |> [x: &'a int, y: &'b int, z: &'c int] |> &'c int {
  if (@x == 2) {
    |> x;
  }
  if (@y == 2) {
    |> y;
  }
  |> z;
}

pipe main |> [] |> [] |> unit {
  int x <| 4;
  while (x < 5) {
    int y <| 4;
    loop 0 -> 5 as z {
      test <| [&x, &y, &z];
    }
  }
  |> ();
}
