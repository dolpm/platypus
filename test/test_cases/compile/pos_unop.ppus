pipe main |> [] |> [] |> unit {
    /* neg */
    int x <| 3;
    printnl <| [-x];

    /* not */
    bool y <| true;
    printnl <| [!y];

    /* ref, deref */
    mut int z <| 5;
    {
      &int a <| &z;
      printnl <| [@a];
    }
    {
      ~&int a <| ~&z;
      printnl <| [@a];
    }

    |> ();
}