pipe main |> [] |> [] |> unit {
  box[int] x <| Box_new <| [2048];
  mut box[int] y <| %x;

  {
    ~&int unboxed <| Box_unbox_mut <| [~&y];
    unboxed <| 4028;
  }

  Printnl <| [Box_unbox <| [&y]];
  Printnl <| [Box_unbox <| [&x]];

  |> ();
}