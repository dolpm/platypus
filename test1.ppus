pipe main |> [] |> [] |> unit {
  mut vector[int] x <| Vector_new <| [];
  Vector_push <| [~&x, 2048];

  mut vector[int] y <| %x;

  {
      ~&int y_setter <| Vector_get_mut <| [~&y, 0];
      y_setter <| 4028;
  }

  
  Printnl <| [Vector_get <| [&y, 0]];
  Printnl <| [Vector_get <| [&x, 0]];
  |> ();
}