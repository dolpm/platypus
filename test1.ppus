pipe inner |'a, 'b| [bass: &'a int, rock: &'b int] |> &'b int {   
    if (@bass > @rock) {
      |> bass;
    }

    |> rock;
}

pipe outer |'c, 'd| [mic: &'c int, treble : &'d int] |> &'d int {       
    int x = 4;

    &int shinola = inner <| [mic, treble];

    |> shinola;
}

