thing MyInnerThing <| {
  im_a_float: float
}

thing MyThing <| {
  inner: MyInnerThing,
  im_an_int: int
}

pipe main |> [] |> [] |> unit {
  MyInnerThing t_inner <| MyInnerThing {
      im_a_float: 17.38
  };

  mut MyThing t <| MyThing {
    inner: t_inner,
    im_an_int: 10
  };

  ~&float z <| ~&t[inner][im_a_float];

  Printnl <| [z];

  |> ();
}